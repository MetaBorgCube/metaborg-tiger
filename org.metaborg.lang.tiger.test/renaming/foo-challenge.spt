module renaming/foo-challenge

language Tiger

test Foo Challenge [[
let
  type foo = {
    foo : string
  }
  function foo (foo: [[foo]]) = (
    let
      var foo := foo.foo
    in
      print(foo)
    end
  )
in
    foo(foo{foo = "foo"})
end
]] run rename-test(|#1, "bar") to [[
let
  type bar = {
    foo : string
  }
  function foo (foo: bar) = (
    let
      var foo := foo.foo
    in
      print(foo)
    end
  )
in
    foo(bar{foo = "foo"})
end
]]